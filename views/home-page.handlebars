{{#if status}}
    <h1>Title: {{postData.title}} <br>
        Content: {{postData.comment}} <br>
        By: {{userData.user_name}} <br>
        Date Created: {{postData.createdAt}} <br>
    </h1>


{{else}}
    <div id="postContainer">
        {{#each posts}}
        <h1 class="post" id="{{id}}">
            Title {{title}} <br>
            Date Created: {{createdAt}}
        </h1>
        {{/each}}
    </div>
    <script src="./js/viewPost.js"></script>
{{/if}}